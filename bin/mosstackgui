#!/usr/bin/python
# -*- coding: utf-8 -*-

"""
Created on 28.4.2014

@author: Mikko Laine

Graphical user interface for pyAstroStack.

"""

import sys

try:
    from PyQt5.QtWidgets import QApplication, QMainWindow
except ImportError:
    print("No PyQt5 found. Check your installation.")
from mosstack import mosstack_gui


class UserInterface():

    def __init__(self):
        app = QApplication(sys.argv)
        window = QMainWindow()
        ui = mosstack_gui.Ui()
        ui.setupUi(window)
        ui.setupManual(window)

        window.show()
        app.exec_()


if __name__ == "__main__":
    UserInterface()
